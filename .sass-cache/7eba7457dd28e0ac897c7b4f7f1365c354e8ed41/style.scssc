:@template"`(/* =============================================================================
    STYLE
   ========================================================================== */

  /* FONTS ================================================================== */

  @import url("http://fonts.googleapis.com/css?family=Oswald:400,700,300");

  @font-face {
  font-family: 'TradeGothicBoldCondensedNo.20';
    src: url('../fonts/tradegothic-boldcondtwenty.eot');
    src: url('../fonts/tradegothic-boldcondtwenty.eot') format('embedded-opentype'),
         url('../fonts/tradegothic-boldcondtwenty.woff') format('woff'),
         url('../fonts/tradegothic-boldcondtwenty.ttf') format('truetype'),
         url('../fonts/tradegothic-boldcondtwenty.svg#TradeGothicBoldCondensedNo.20') format('svg');
  }

  /* END FONTS ============================================================== */

    a,
    a:focus,
    a:visited{
      outline: none;
    }

   ul{
    padding: 0;
    margin: 0;
    list-style-type: none;
   }


    body{
        font-size: 100%;
        font-family: Georgia;
        color: #ffffff;
    }

    .row{
        width: 100%;
    }

    .one-column .column{
      width: 50%;
    }

    .two-column .column{
        width: 48%;
        margin: 1%;
        background: white;
        float: left;
    }

    p{
        font-size: 0.8em;
        text-shadow: 0px 0px 8px #000000;
    }

    h1, h2, h3, h4{
      font-family: "TradeGothicBoldCondensedNo.20";
      font-weight: normal;
      text-transform: uppercase;
      color: #ffffff;
      text-shadow: 5px 5px 10px #000000;
    }


    /* GENERAL STYLES ======================================================= */
    body{
      background: #000000;
    }

    .main-frame-left,
    .main-frame-right{
      display: block;
      height: 100%;
      position: fixed;
      top: 0;
      width: 105px;
      display: none;
    }

    .main-frame-left{
      left: 0;
      background: transparent url('../img/frame/main_frame_LEFT_pattern.png') repeat-y top left;
    }

    .main-frame-right{
      right: 0;
      background: transparent url('../img/frame/main_frame_RIGHT_pattern.png') repeat-y top right;
    }
    /* END GENERAL STYLES =================================================== */




    /* HEADER STYLES ======================================================== */
    .header{
      position: fixed;
      top: 0;
      width: 100%;
      height: 50px;
      background: #000000;

      z-index: 100;



      .nav-collapse,
      .social-icons{
        position: absolute;
      }

      .nav-collapse{
        top: 19px;
        left: 2%;

        h2{
          background: #000000;
          color: #F7F6CD;
          cursor: pointer;
          float: left;
          font-size: 1.9em;
          height: auto;
          line-height: 1em;
          margin: 0;
          padding: 2px 10px 0;
          text-align: center;
          width: 101px;
        }
      }

      .social-icons{
        background: none repeat scroll 0 0 #000000;
        margin: 0;
        right: -150px;
        top: 150px;
        border-left: 10px solid #ffffff;
        width: 120px;
        padding: 20px;
        background: #d9561c;

        -webkit-transition: all 0.4s ease-in-out;
        transition: all 0.4s ease-in-out;

        &:hover{
          -webkit-transition: all 0.3s ease-in-out;
          transition: all 0.3s ease-in-out;
          -webkit-transform: translateX(-150px);
          -ms-transform: translateX(-150px);
          transform: translateX(-150px);
        }

        h2{
          display: none;
        }

        a{
          padding: 0;
          float: left;
          height: 25px;
          margin-left: 10px;
          text-indent: -99999px;
          width: 25px;

        }

        .bohemestudio-icon{
          width: 120px;
          height: 37px;
          margin: 0 0 10px;
          padding: 0;
          text-indent: 0;

          span{
            display: none;
          }
        }

        .facebook-icon{
          background: transparent url("../img/header/icons_sprite.png") no-repeat scroll 0 0;
        }

        .instagram-icon{
          background: transparent url("../img/header/icons_sprite.png") no-repeat scroll  0 -35px;
        }

        .twitter-icon{
          background: transparent url("../img/header/icons_sprite.png") no-repeat scroll  0 -70px;
        }

      }

      .campaign-slogan{
        text-align: center;

        img{
          background: #000000;
          padding: 12px 20px 0 20px;
        }
      }

      .main-nav{
        ul{

          margin-top: 0px;

          width: 100%;
          float: left;

          padding: 10px 0 5px 0 ;

          background-color: rgba(0, 0, 0, 0.9);
        }

        ul li{
          float: left;
          width: 20%;

          a{
            font-family: "TradeGothicBoldCondensedNo.20";
            font-size: 1.3em;
            text-transform: uppercase;
            color: #ffffff;
            display: block;
            text-align: center;
            text-decoration: none;

            &:hover{
              color: #d9561c;
            }
          }
        }
      }
    }
    /* END HEADER STYLES ==================================================== */




    /* PRODUCT PAGES STYLES ================================================= */
    .tabs-menu{
      width: 100%;

      ul{

        width: 100%;

        li{
          width: 33%;
          float: left;
        }
      }


      .tab-nav{
        width: 90%;
        display: block;
        background: #000000;
        color: #ffffff;
        text-align: center;
        padding: 20px 0;

        z-index: 100;
        margin: 20%;
      }
    }

    .stick{
      position: fixed;
      top: 0;
      left: 0;

      .tab-nav{
        background: #ffffff;
        color: #000000;
      }
    }

    .content-tabs{

      h2{
        font-size: 2em;
        text-shadow: 0px 0px 8px #000000;
      }

      p{
        text-shadow: 3px 3px 5px #000000;
      }

      .tab{
        display: block;
        position: relative;
        width: 100%;
        text-align: center;

        .row{
          z-index: 10;
        }

        .tab-nav{
          z-index: 100;
          position: fixed;
          top: 150px;
          left: 0px;
          text-align: left;

          a{
            font-family: "TradeGothicBoldCondensedNo.20";
            text-decoration: none;
            text-align: right;
            color: #165399;
            background: #ffffff;
            padding: 10px 20px;
            margin-bottom: 5px;
            display: block;
            width: 170px;
            border-right: 10px solid #ffffff;
            position: relative;
            left: -190px;
            -webkit-transition: all 0.4s ease-in-out;
            transition: all 0.4s ease-in-out;

            background: #bcd3e5; /* Old browsers */
            background: -moz-linear-gradient(left,  #bcd3e5 0%, #165399 100%); /* FF3.6+ */
            background: -webkit-gradient(linear, left top, right top, color-stop(0%,#bcd3e5), color-stop(100%,#165399)); /* Chrome,Safari4+ */
            background: -webkit-linear-gradient(left,  #bcd3e5 0%,#165399 100%); /* Chrome10+,Safari5.1+ */
            background: -o-linear-gradient(left,  #bcd3e5 0%,#165399 100%); /* Opera 11.10+ */
            background: -ms-linear-gradient(left,  #bcd3e5 0%,#165399 100%); /* IE10+ */
            background: linear-gradient(to right,  #bcd3e5 0%,#165399 100%); /* W3C */






            &:hover,
            &:active{
              color: #000000;

              text-shadow: none;
              -webkit-transition: all 0.3s ease-in-out;
              transition: all 0.3s ease-in-out;
              -webkit-transform: translateX(170px);
              -ms-transform: translateX(170px);
              transform: translateX(170px);
            }

            &.tab1{
              background: #166397;
            }

            &.tab2{
              background: #0087B3;
            }

            &.tab3{
              background: #4AAFCB;
            }

            &.tab4{
              background: #94C3D5;
            }

            &.tab5{
              background: #CAE1E9;
            }
          }
        }

        .youtube-background{
          position: absolute;
          top: 0;
          left: 0;
          overflow: hidden;

          z-index: -99;
          z-index: 1;
        }
      }
    }

    .tab-arrow-nav{
      position: fixed;
      bottom: 10px;
      width: 100%;
      z-index: 10;

      ul{
        width: 100%;
        float: left;

        li{
          float: left;
          width: 50%;
          display: block;
        }
      }

      a{
        width: 10%;
        background: #046695;
        font-family: "TradeGothicBoldCondensedNo.20";
        color: #ffffff ;
        text-decoration: none;
        text-align: center;
        padding: 10px 1%;

        &:hover{
          background: #ffffff;
          color: #046695;
        }
      }


      .prev{
        display: none;
        float: right;
      }

      .next{
        float: left;
      }
    }

    /* Backgrounds */
    #tab1{
      background: transparent url('../img/backgrounds/the_dark_knight_bg1.jpg') no-repeat center center;
      background-size: 100%;
    }
    #tab3{
      background: transparent url('../img/backgrounds/the_dark_knight_bg3.jpg') no-repeat center center;
      background-size: 100%;
    }
    #tab4{
      background: transparent url('../img/backgrounds/the_dark_knight_bg4.jpg') no-repeat center center;
      background-size: 100%;
    }
    #tab5{
      background: transparent url('../img/backgrounds/the_dark_knight_bg5.jpg') no-repeat center center;
      background-size: 100%;
    }

    /* END PRODUCT PAGES STYLES ============================================= */


/* ----------------------------------- ENHANCED VIEW - TABLET ----------------------------------- */
@media all and (min-width: 767px){

  @import "style_tablet.scss";

}


/* ----------------------------------- ENHANCED VIEW - DESKTOP ----------------------------------- */
@media all and (min-width: 940px){

  @import "style_desktop.scss";

}


/* FONTS OLDER IE */
.lt-ie9{
  h1, h2, h3, h4,
  .header .main-nav ul li a{
     font-family: 'Oswald', sans-serif;
  }
}

