:@template"Ý#/* =============================================================================
    STYLE
   ========================================================================== */

  /* FONTS ================================================================== */

@import url(http://fonts.googleapis.com/css?family=Marck+Script);

  /* END FONTS ============================================================== */

    a,
    a:focus,
    a:visited{
      outline: none;
    }

   ul{
    padding: 0;
    margin: 0;
    list-style-type: none;
   }


    body{
        font-size: 100%;
        font-family: Georgia;
        color: #ffffff;
        overflow-x: hidden;
    }

    .row{
        width: 100%;
    }

    .one-column .column{
      width: 100%;
    }

    .two-column .column{
        width: 48%;
        margin: 1%;
        background: white;
        float: left;
    }

    p{
        font-size: 0.8em;
        text-shadow: 0px 0px 8px #000000;
    }

    h1, h2, h3, h4{
      font-family: "TradeGothicBoldCondensedNo.20";
      font-weight: normal;
      text-transform: uppercase;
      color: #ffffff;
      text-shadow: 5px 5px 10px #000000;
    }


    /* GENERAL STYLES ======================================================= */
    body{
      background: #000000;
    }

    /* END GENERAL STYLES =================================================== */




    /* HEADER STYLES ======================================================== */
    .header{
      position: fixed;
      top: 0;
      width: 100%;
      height: 50px;
      background: rgba(0, 0, 0, 0.7);
      -webkit-box-shadow:  0px 10px 10px 0px rgba(0, 0, 0, 0.5);
      box-shadow:  0px 10px 10px 0px rgba(0, 0, 0, 0.5);

      z-index: 100;



      .nav-collapse,
      .social-icons{
        position: absolute;
      }

      .nav-collapse{
        top: 19px;
        left: 2%;

        h2{
          background: #000000;
          color: #F7F6CD;
          cursor: pointer;
          float: left;
          font-size: 1.9em;
          height: auto;
          line-height: 1em;
          margin: 0;
          padding: 2px 10px 0;
          text-align: center;
          width: 101px;
        }
      }

      .social-icons{
        background: none repeat scroll 0 0 #000000;
        margin: 0;
        right: -150px;
        top: 150px;
        border-left: 10px solid #ffffff;
        width: 120px;
        padding: 20px;
        background: #d9561c;

        -webkit-transition: all 0.4s ease-in-out;
        transition: all 0.4s ease-in-out;

        &:hover{
          -webkit-transition: all 0.3s ease-in-out;
          transition: all 0.3s ease-in-out;
          -webkit-transform: translateX(-150px);
          -ms-transform: translateX(-150px);
          transform: translateX(-150px);
        }

        h2{
          display: none;
        }

        a{
          padding: 0;
          float: left;
          height: 25px;
          margin-left: 10px;
          text-indent: -99999px;
          width: 25px;

        }

        .bohemestudio-icon{
          width: 120px;
          height: 37px;
          margin: 0 0 10px;
          padding: 0;
          text-indent: 0;

          span{
            display: none;
          }
        }

        .facebook-icon{
          background: transparent url("../img/header/icons_sprite.png") no-repeat scroll 0 0;
        }

        .instagram-icon{
          background: transparent url("../img/header/icons_sprite.png") no-repeat scroll  0 -35px;
        }

        .twitter-icon{
          background: transparent url("../img/header/icons_sprite.png") no-repeat scroll  0 -70px;
        }

      }

      .campaign-slogan{
        text-align: center;

        img{
          background: #000000;
          padding: 12px 20px 0 20px;
        }
      }

      .main-nav{
        ul{

          margin-top: 0px;
          width: 100%;
          float: left;
          padding: 10px 0 5px 0 ;

        }

        ul li{
          float: left;
          width: 20%;

          a{
            font-family: "Verdana";
            font-size: 0.8em;
            text-transform: uppercase;
            color: #ffffff;
            display: block;
            text-align: center;
            text-decoration: none;

            &.active,
            &:hover{
              color: #d9561c;
            }
          }
        }
      }
    }
    /* END HEADER STYLES ==================================================== */




    /* MAIN PAGE STYLES ================================================= */
    .tabs-menu{
      width: 100%;

      ul{

        width: 100%;

        li{
          width: 33%;
          float: left;
        }
      }


      .tab-nav{
        width: 90%;
        display: block;
        background: #000000;
        color: #ffffff;
        text-align: center;
        padding: 20px 0;

        z-index: 100;
        margin: 20%;
      }
    }

    .stick{
      position: fixed;
      top: 0;
      left: 0;

      .tab-nav{
        background: #ffffff;
        color: #000000;
      }
    }


    .tab-nav{
      z-index: 100;
      position: fixed;
      top: 50px;
      left: 0px;
      text-align: left;
      width: 100%;
      background: rgba(0,0,0,0.2);

      a{

        font-size: 0.9em;

        display: block;
        width: 20%;
        float: left;
        font-family: 'Marck Script', cursive;
        text-decoration: none;
        padding: 10px 0;
        color: #ffffff;

        position: relative;

        text-align:center;
        /* Left - Top - Grossir*/
        text-shadow:
          1px 1px 1px rgba(0,0,0,1),
          1px 1px 0 #2a658b,
          2px 2px 0 #29638a,
          3px 3px 0 #286188,
          4px 4px 0 #275f85,
          5px 5px 0 #265d83,
          6px 6px 0 #255a80,
          7px 7px 0 #24577d,
          8px 8px 0 #235479,
          9px 9px 0 #215075,
          10px 10px 0 #1f4c71,
          11px 11px 0 #1e486c,
          12px 12px 1px rgba(28,67,103,0.5),
          14px 14px 12px rgba(5,13,20,0.5);



        -webkit-transition: all 0.3s ease-in-out;
        transition: all 0.3s ease-in-out;

        &:hover,
        &:active{
          color: #ffffff;

          color: #FFF;
          text-shadow: #58E 1px 1px, #58E 2px 2px, #58E 3px 3px, #58E 4px 4px, #58E 5px 5px, #58E 6px 6px;
          left: 5px;
          top: 4px;


          text-shadow: none;
          -webkit-transition: all 0.3s ease-in-out;
          transition: all 0.3s ease-in-out;
        }

        &.tab1{

        }

        &.tab2{

        }

        &.tab3{

        }

        &.tab4{

        }

        &.tab5{

        }
      }
    }

    .content-tabs{

      h2{
        font-size: 2em;
        text-shadow: 0px 0px 8px #000000;
        display: none;
      }

      p{
        text-shadow: 3px 3px 5px #000000;
      }

      .tab{
        display: block;
        position: relative;
        width: 100%;
        text-align: center;

        .row{
          z-index: 10;
        }
      }
    }

    .tab-arrow-nav{
      position: fixed;
      bottom: 0;
      width: 100%;
      z-index: 10;

      ul{
        width: 100%;
        float: left;

        li{

          display: block;
        }
      }

      a{
        width: 60px;
        height: 60px;
        text-indent: 9999px;
        display: block;

        &:hover{

        }
      }


      .prev{
        display: none;
        float: left;
        background: transparent url(../img/retro-backgrounds/next_prev_sprite.png) no-repeat 0 -60px;
      }

      .next{
        float: right;
        background: transparent url(../img/retro-backgrounds/next_prev_sprite.png) no-repeat 0 0;
      }
    }

    /* Backgrounds */
    .tab{

      position: relative;
      background: none;

      a{
        display: block;
        text-align: center;

        img{
          width: 100%;
          height: auto;
          display: block;
          max-width: 500px;
          max-height: 500px;
          margin: 0 auto;
        }

      }
    }


    .fullsize-background{

      width: 100%;
      height: 100%;
      z-index: -99998;

      img{
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: -99999;
        overflow: hidden;
        margin: 0;
        padding: 0;
      }
    }

    #tab1{

    }
    #tab2{

    }
    #tab3{

    }
    #tab4{

    }
    #tab5{

    }

    /* END PRODUCT PAGES STYLES ============================================= */


