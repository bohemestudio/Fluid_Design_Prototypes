:@template"»!/* =============================================================================
    STYLE
   ========================================================================== */

  /* FONTS ================================================================== */

@import url(http://fonts.googleapis.com/css?family=Luckiest+Guy);

  /* END FONTS ============================================================== */

    a,
    a:focus,
    a:visited{
      outline: none;
    }

   ul{
    padding: 0;
    margin: 0;
    list-style-type: none;
   }


    body{
        font-size: 100%;
        font-family: Georgia;
        color: #ffffff;
        overflow-x: hidden;
    }

    .row{
        width: 100%;
    }

    .one-column .column{
      width: 100%;
    }

    .two-column .column{
        width: 48%;
        margin: 1%;
        background: white;
        float: left;
    }

    p{
        font-size: 0.8em;
        text-shadow: 0px 0px 8px #000000;
    }

    h1, h2, h3, h4{
      font-family: Verdana;
      font-weight: normal;
      text-transform: uppercase;
      color: #ffffff;
      text-shadow: 5px 5px 10px #000000;
    }


    /* GENERAL STYLES ======================================================= */
    body{
      background: #000000;
    }

    /* END GENERAL STYLES =================================================== */




    /* HEADER STYLES ======================================================== */
    .header{
      position: fixed;
      top: 0;
      width: 100%;
      height: 50px;
      background: rgba(0, 0, 0, 0.7);
      -webkit-box-shadow:  0px 10px 10px 0px rgba(0, 0, 0, 0.5);
      box-shadow:  0px 10px 10px 0px rgba(0, 0, 0, 0.5);

      z-index: 100;



      .nav-collapse,
      .social-icons{
        position: absolute;
      }

      .nav-collapse{
        top: 19px;
        left: 2%;

        h2{
          background: #000000;
          color: #F7F6CD;
          cursor: pointer;
          float: left;
          font-size: 1.9em;
          height: auto;
          line-height: 1em;
          margin: 0;
          padding: 2px 10px 0;
          text-align: center;
          width: 101px;
        }
      }

      .social-icons{
        background: none repeat scroll 0 0 #000000;
        margin: 0;
        right: -150px;
        top: 150px;
        border-left: 10px solid #ffffff;
        width: 120px;
        padding: 20px;
        background: #d9561c;

        -webkit-transition: all 0.4s ease-in-out;
        transition: all 0.4s ease-in-out;

        &:hover{
          -webkit-transition: all 0.3s ease-in-out;
          transition: all 0.3s ease-in-out;
          -webkit-transform: translateX(-150px);
          -ms-transform: translateX(-150px);
          transform: translateX(-150px);
        }

        h2{
          display: none;
        }

        a{
          padding: 0;
          float: left;
          height: 25px;
          margin-left: 10px;
          text-indent: -99999px;
          width: 25px;

        }

        .bohemestudio-icon{
          width: 120px;
          height: 37px;
          margin: 0 0 10px;
          padding: 0;
          text-indent: 0;

          span{
            display: none;
          }
        }

        .facebook-icon{
          background: transparent url("../img/header/icons_sprite.png") no-repeat scroll 0 0;
        }

        .instagram-icon{
          background: transparent url("../img/header/icons_sprite.png") no-repeat scroll  0 -35px;
        }

        .twitter-icon{
          background: transparent url("../img/header/icons_sprite.png") no-repeat scroll  0 -70px;
        }

      }

      .campaign-slogan{
        text-align: center;

        img{
          background: #000000;
          padding: 12px 20px 0 20px;
        }
      }

      .main-nav{
        ul{

          margin-top: 0px;
          width: 100%;
          float: left;
          padding: 10px 0 5px 0 ;

        }

        ul li{
          float: left;
          width: 20%;

          a{
            font-family: "Verdana";
            font-size: 0.8em;
            text-transform: uppercase;
            color: #ffffff;
            display: block;
            text-align: center;
            text-decoration: none;

            &.active,
            &:hover{
              color: #d9561c;
            }
          }
        }
      }
    }
    /* END HEADER STYLES ==================================================== */




    /* MAIN PAGE STYLES ================================================= */
    .tabs-menu{
      width: 100%;

      ul{

        width: 100%;

        li{
          width: 33%;
          float: left;
        }
      }


      .tab-nav{
        width: 90%;
        display: block;
        background: #000000;
        color: #ffffff;
        text-align: center;
        padding: 20px 0;

        z-index: 100;
        margin: 20%;
      }
    }

    .stick{
      position: fixed;
      top: 0;
      left: 0;

      .tab-nav{
        background: #ffffff;
        color: #000000;
      }
    }


    .tab-nav{
      z-index: 100;
      position: fixed;
      top: 50px;
      left: 0px;
      text-align: left;
      width: 100%;
      background: rgba(255,255,255,0.4);

      a{

        font-size: 0.9em;

        display: block;
        width: 20%;
        float: left;
        font-family: 'Luckiest Guy', cursive;
        text-decoration: none;
        padding: 10px 0;
        color: #ffffff;

        position: relative;

        text-align:center;
        text-shadow: 5px 5px 0px black;



        -webkit-transition: all 0.3s ease-in-out;
        transition: all 0.3s ease-in-out;

        &:hover,
        &:active{
          color: #ffffff;

          color: #d9561c;

          -webkit-transition: all 0.3s ease-in-out;
          transition: all 0.3s ease-in-out;
        }

        &.tab1{

        }

        &.tab2{

        }

        &.tab3{

        }

        &.tab4{

        }

        &.tab5{

        }
      }
    }

    .content-tabs{

      h2{
        font-size: 2em;
        text-shadow: 0px 0px 8px #000000;
        display: none;
      }

      p{
        text-shadow: 3px 3px 5px #000000;
      }

      .tab{
        display: block;

        width: 100%;
        text-align: center;


        .row{
          z-index: 10;
          position: relative;
        }

        a{
          display: block;
          text-align: center;

          img{
            width: 100%;
            height: auto;
            display: block;
            max-width: 500px;
            max-height: 500px;
            margin: 0 auto;
          }

        }


      }
    }

    .tab-arrow-nav{
      position: fixed;
      bottom: 0;
      width: 100%;
      z-index: 10;

      ul{
        width: 100%;
        float: left;

        li{

          display: block;
        }
      }

      a{
        width: 60px;
        height: 60px;
        text-indent: 9999px;
        display: block;

        &:hover{

        }
      }


      .prev{
        display: none;
        float: left;
        background: transparent url(../img/comic-backgrounds/next_prev_sprite.png) no-repeat 0 -60px;
      }

      .next{
        float: right;
        background: transparent url(../img/comic-backgrounds/next_prev_sprite.png) no-repeat 0 0;
      }
    }

    /* Backgrounds */

/*
    .fullsize-background{

      width: 100%;
      height: 100%;
      z-index: -99998;

      img{
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: -99999;
        overflow: hidden;
        margin: 0;
        padding: 0;
      }
    }*/

    #tab1{
      background: transparent url("../img/comic-backgrounds/comic01.gif") no-repeat top left;
      background-size: 100%;
    }
    #tab2{
      background: transparent url("../img/comic-backgrounds/comic02.gif") no-repeat top left;
      background-size: 100%;
    }
    #tab3{
      background: transparent url("../img/comic-backgrounds/comic03.gif") no-repeat top left;
      background-size: 100%;
    }
    #tab4{

    }
    #tab5{

    }

    /* END PRODUCT PAGES STYLES ============================================= */


/* ----------------------------------- ENHANCED VIEW - TABLET ----------------------------------- */
@media all and (min-width: 767px){

  @import "style_tablet.scss";

}


/* ----------------------------------- ENHANCED VIEW - DESKTOP ----------------------------------- */
@media all and (min-width: 940px){

  @import "style_desktop.scss";

}


